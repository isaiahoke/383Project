<%- include ('partials/header') %>
<style>
	.day-wethear-item {
    cursor: pointer;
}
#flash-container{
	position: relative;
}
p#flash {
    z-index: 100;
    position: relative;
}
</style>
<!-- Main Header Weather -->
<span id="flash-container">
	<% if (messages.error != '' && messages.error != undefined && messages.error != null){ %>
		<p style="background: white; padding: 10px; border-left: 2px solid #a00202;"
		class="alert-text text-prompt-pop text-error text-bg animate__animated animate__bounceInLeft" id="flash"><%= messages.error %></p>
					 
	<% } else if (messages.success != '' && messages.success != undefined && messages.success != null) {%>
		<p style="background: white; padding: 10px; border-left: 2px solid #66affa;"
		class="alert-text text-prompt-pop text-success text-bg animate__animated animate__bounceInLeft" id="flash"><%= messages.success %></p>
	<%}%>
 </span>
	
	
<!-- ... end Main Header Weather -->



<!-- Widget Weather -->

<div class="container" style="margin-top: 40px;">
	<div class="row">
		<div class="col col-xl-12 order-xl-2 col-lg-12 order-lg-1 col-md-12 col-sm-12 col-12">
			<div class="ui-block">
				<div class="ui-block-title">
					<h6 class="title">All users</h6>
				</div>
				<div class="ui-block-content">

					<!-- Form Weather -->
					<table class="table table-dark">
						<thead>
						  <tr>
							<th scope="col">#</th>
							<th scope="col">Fullnames</th>
							<th scope="col">Email</th>
							<th scope="col">City</th>
						  </tr>
						</thead>
						<tbody>
						<% userlist.forEach((user, index)=>{%>
							<tr>
								<th scope="row"><%=index + 1%></th>
								<td><%=user.fullnames%></td>
								<td><%=user.username%></td>
								<td><%=user.location%></td>
							  </tr>
						<%})%>
						 
						
						</tbody>
					  </table>
					
					<!-- ... end Form Weather -->
				</div>
			</div>

		

		</div>

	
	</div>
</div>


<!-- ... end Widget Weather -->

<script>
	document.addEventListener("DOMContentLoaded", function(event) { 
	  setTimeout(function(){
        document.getElementById('flash').remove()
      }, 10000);
    });
</script>


<%- include('partials/footer') %>
